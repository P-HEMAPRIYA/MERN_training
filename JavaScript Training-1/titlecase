function titlecase(str){
    const words=str.toLowerCase().split(" ");
    let  titlecasedWords=[];
    
    for(let word of words){
       titlecasedWords.push(word[0].toUpperCase()+word.slice(1));
       
    }
    return titlecasedWords.join(" ");

}
console.log(titlecase("the Quick brown fox"));

  